    "@tiptap/extension-mention": "*",
    "@tiptap/extension-placeholder": "*",
    "@tiptap/extension-text-style": "*",
    "@tiptap/extension-underline": "*",
    "@tiptap/starter-kit": "*",
    "@tiptap/vue-2": "*",
    "d3": "5.7.0",
    "gulp-run": "^1.7.1",
    "hast-util-to-html": "^8.0.0",
